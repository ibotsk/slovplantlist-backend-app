{
  "name": "genus",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "authors": {
      "type": "string",
      "required": true
    },
    "vernacular": {
      "type": "string"
    },
    "idFamily": {
      "type": "number",
      "required": true,
      "mysql": {
        "columnName": "id_family"
      }
    },
    "idFamilyApg": {
      "type": "number",
      "required": true,
      "mysql": {
        "columnName": "id_family_apg"
      }
    }
  },
  "validations": [],
  "relations": {
    "family": {
      "type": "belongsTo",
      "model": "family",
      "foreignKey": "idFamily"
    },
    "family-apg": {
      "type": "belongsTo",
      "model": "family-apg",
      "foreignKey": "idFamilyApg"
    },
    "nomenclatures": {
      "type": "hasMany",
      "model": "nomenclature",
      "foreignKey": "idGenus"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "editor",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
